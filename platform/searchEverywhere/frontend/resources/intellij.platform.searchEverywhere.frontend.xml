<idea-plugin package="com.intellij.platform.searchEverywhere.frontend">
  <dependencies>
    <plugin id="com.intellij.platform.experimental.frontend"/>
    <module name="intellij.platform.searchEverywhere"/>
  </dependencies>
  <actions>
    <action id="SearchEverywhereActionsInitiatorAction"
            class="com.intellij.platform.searchEverywhere.frontend.actions.SeActionsInitiatorAction"
            text="Search Actions in SE 2.0"/>
  </actions>

  <extensions defaultExtensionNs="com.intellij">
    <searchEverywhere.tabProvider implementation="com.intellij.platform.searchEverywhere.frontend.mocks.SeTabProviderMockAlphaLocal"/>
    <searchEverywhere.tabProvider implementation="com.intellij.platform.searchEverywhere.frontend.mocks.SeTabProviderMockBetaLocal"/>
    <searchEverywhere.tabProvider implementation="com.intellij.platform.searchEverywhere.frontend.mocks.SeTabProviderMockCharlieRemote"/>
    <searchEverywhere.tabProvider implementation="com.intellij.platform.searchEverywhere.frontend.mocks.SeTabProviderMockDeltaMixed"/>
    <searchEverywhere.tabProvider implementation="com.intellij.platform.searchEverywhere.frontend.mocks.files.SeFilesTabProvider"/>
  </extensions>
</idea-plugin>