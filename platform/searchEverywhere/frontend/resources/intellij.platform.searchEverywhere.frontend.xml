<idea-plugin package="com.intellij.platform.searchEverywhere.frontend">
  <dependencies>
    <plugin id="com.intellij.platform.experimental.frontend"/>
    <module name="intellij.platform.searchEverywhere"/>
  </dependencies>
  <actions>
    <action id="SearchEverywhereActionsInitiatorAction"
            class="com.intellij.platform.searchEverywhere.frontend.actions.SearchEverywhereActionsInitiatorAction"
            text="Search Actions in SE 2.0"/>
  </actions>

  <extensions defaultExtensionNs="com.intellij">
    <searchEverywhere.tabProvider implementation="com.intellij.platform.searchEverywhere.frontend.mocks.SearchEverywhereTabProviderMockAlphaLocal"/>
    <searchEverywhere.tabProvider implementation="com.intellij.platform.searchEverywhere.frontend.mocks.SearchEverywhereTabProviderMockBetaLocal"/>
    <searchEverywhere.tabProvider implementation="com.intellij.platform.searchEverywhere.frontend.mocks.SearchEverywhereTabProviderMockCharlieRemote"/>
    <searchEverywhere.tabProvider implementation="com.intellij.platform.searchEverywhere.frontend.mocks.SearchEverywhereTabProviderMockDeltaMixed"/>
  </extensions>
</idea-plugin>